    "dev:clean": "pnpm clean:next && pnpm dev",
    "dev:legacy": "node server.js",
    "prebuild": "node scripts/sync-version.js",
    "build": "prisma generate --no-engine && next build --webpack",
    "prestart": "node scripts/sync-version.js",
    "start": "next start",
