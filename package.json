    "predev:verbose": "node scripts/sync-version.js",
    "dev:verbose": "NODE_OPTIONS='--trace-warnings' DEBUG=next:* next dev --webpack --experimental-https --experimental-https-key ./localhost-key.pem --experimental-https-cert ./localhost.pem 2>&1 | tee dev.log",
    "predev:debug": "node scripts/sync-version.js",
